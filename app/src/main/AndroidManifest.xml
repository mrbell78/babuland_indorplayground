            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.BabulandpointsActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.QuizresultActivity"
